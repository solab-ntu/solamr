<?xml version="1.0"?>
<launch>
    <arg name="robot_name" default="car2" />
    <arg name="role" default="follower" />
    <!-- MQTT -->
    <node pkg="ros_mqtt_bridge" type="$(arg role).py"  name="ros_mqtt_bridge" output='screen'/>

    <!-- Shelf Detector change paran -->
    <node pkg="rosservice" type="rosservice" name="Shelf_Detector_Param_set" args="call --wait StringSrv '$(find obstacle_detector)/param/double_AMR.yaml'" />
</launch>
